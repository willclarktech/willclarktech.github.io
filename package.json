{
  "name": "willclark-inferno",
  "version": "2.0.0",
  "private": true,
  "devDependencies": {
    "inferno-scripts": "3.0.0"
  },
  "dependencies": {
    "babel-plugin-inferno": "^3.2.0",
    "inferno": "^3.0.6",
    "inferno-component": "^3.0.6"
  },
  "scripts": {
    "start": "inferno-scripts start",
    "build": "inferno-scripts build",
    "postbuild": "cp CNAME ./build && cp README.md ./build",
